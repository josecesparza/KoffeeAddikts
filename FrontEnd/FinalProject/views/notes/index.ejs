<% include ../partials/header %>

<div id="tags">
    <div class="tag">tag1</div>
    <div class="tag">tag2</div>
    <div class="tag">tag3</div>
    <div class="tag">tag4</div>
</div>

<% if(notes){ %>
    <% notes.forEach(function(note){ %>
        <div>
            <h4><%= note.title %></h4>
            <p><%= note.content %></p>
            <a href="/notes/<%= note._id %>">More Info</a>
        </div>
    <% }) %>
<% } %>

